<?xml version="1.0" encoding="utf-8"?>
<Application Title="TestXafSolution">
  <ActionDesign>
    <Actions>
      <Action Id="ClearFieldsAction" ImageName="Action_Clear" Shortcut="Control+C" />
      <Action Id="FindFieldDataAction" SelectionDependencyType="RequireSingleObject" />
      <Action Id="FullTextSearch" PaintStyle="Default" DefaultItemMode="LastExecutedItem" />
    </Actions>
  </ActionDesign>
  <BOModel>
    <Class Name="TestXafSolution.Module.BusinessObjects.TestWork.Area" ImageName="Area">
      <OwnMembers>
        <Member Name="Create_Area" DisplayFormat="{0: ddd, dd MMMM yyyy hh:mm:ss tt}" />
        <Member Name="Delete_Area" DisplayFormat="{0: ddd, dd MMMM yyyy hh:mm:ss tt}" />
      </OwnMembers>
    </Class>
    <Class Name="TestXafSolution.Module.BusinessObjects.TestWork.Cargo" ImageName="Cargo">
      <OwnMembers>
        <Member Name="Create_Cargo" DisplayFormat="{0: ddd, dd MMMM yyyy hh:mm:ss tt}" />
        <Member Name="Delete_Cargo" DisplayFormat="{0: ddd, dd MMMM yyyy hh:mm:ss tt}" />
      </OwnMembers>
    </Class>
    <Class Name="TestXafSolution.Module.BusinessObjects.TestWork.Picket" ImageName="Picket">
      <OwnMembers>
        <Member Name="Name" DataSourceCriteriaProperty="" />
      </OwnMembers>
    </Class>
    <Class Name="TestXafSolution.Module.BusinessObjects.TestWork.Store" ImageName="Store" />
  </BOModel>
  <Options>
    <LayoutManagerOptions RequiredFieldMark="*" />
  </Options>
  <SchemaModules>
    <SchemaModule Name="SystemModule" Version="16.2.3.0" IsNewNode="True" />
  </SchemaModules>
  <Views>
    <ListView Id="Area_LookupListView" Criteria="[Delete_Area] Is Null" />
    <ListView Id="Picket_LookupListView" Criteria="Not Contains([Name], 'Area.Name')" />
  </Views>
</Application>